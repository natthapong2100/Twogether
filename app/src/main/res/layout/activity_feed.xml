<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FeedActivity">

    <ImageView
        android:id="@+id/feedImageViewUP"
        android:layout_width="87dp"
        android:layout_height="61dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="610dp"
        android:layout_marginEnd="9dp"
        android:layout_marginBottom="27dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/createPostImageViewUP"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/viewfeed" />

    <ImageView
        android:id="@+id/createPostImageViewUP"
        android:layout_width="89dp"
        android:layout_height="62dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="610dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="26dp"
        android:onClick="clickCreatePost"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/chatImageViewUP"
        app:layout_constraintStart_toEndOf="@+id/feedImageViewUP"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.545"
        app:srcCompat="@drawable/createpost"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/chatImageViewUP"
        android:layout_width="63dp"
        android:layout_height="56dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="610dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="26dp"
        android:onClick="clickChat"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/userprofileImageViewUP"
        app:layout_constraintStart_toEndOf="@+id/createPostImageViewUP"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/chat"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/userprofileImageViewUP"
        android:layout_width="78dp"
        android:layout_height="64dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="610dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="27dp"
        android:onClick="clickUserProfile"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/chatImageViewUP"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/userprofile"
        tools:ignore="SpeakableTextPresentCheck" />

    <ListView
        android:id="@+id/listView"
        android:layout_width="389dp"
        android:layout_height="579dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="2dp"
        app:layout_constraintBottom_toTopOf="@+id/createPostImageViewUP"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>