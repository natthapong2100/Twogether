<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserProfile">


    <ImageView
        android:id="@+id/createPostImageViewUP"
        android:layout_width="89dp"
        android:layout_height="62dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="610dp"
        android:layout_marginEnd="9dp"
        android:layout_marginBottom="27dp"
        android:onClick="clickCreatePost"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/chatImageViewUP"
        app:layout_constraintStart_toEndOf="@+id/feedImageViewUP"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.478"
        app:srcCompat="@drawable/createpost1"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/feedImageViewUP"
        android:layout_width="87dp"
        android:layout_height="61dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="610dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="27dp"
        android:onClick="feedClick"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/createPostImageViewUP"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/viewfeed1"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/userprofileImageViewUP"
        android:layout_width="78dp"
        android:layout_height="64dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="610dp"
        android:layout_marginBottom="26dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/chatImageViewUP"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.458"
        app:srcCompat="@drawable/userprofile1" />

    <ImageView
        android:id="@+id/chatImageViewUP"
        android:layout_width="63dp"
        android:layout_height="56dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="610dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="27dp"
        android:onClick="clickChat"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/userprofileImageViewUP"
        app:layout_constraintStart_toEndOf="@+id/createPostImageViewUP"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.352"
        app:srcCompat="@drawable/chat1"
        tools:ignore="SpeakableTextPresentCheck" />

    <ListView
        android:id="@+id/listView"
        android:layout_width="375dp"
        android:layout_height="323dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <ImageView
        android:id="@+id/userProfileImage"
        android:layout_width="141dp"
        android:layout_height="151dp"
        android:layout_marginStart="126dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="126dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/profile_gen" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="240dp"
        android:layout_marginEnd="310dp"
        android:fontFamily="@font/allerta"
        android:text="Following :"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/signUpUsernameEditText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="176dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="177dp"
        android:layout_marginBottom="65dp"
        android:fontFamily="@font/allerta"
        android:text="name"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/listView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userProfileImage" />


</androidx.constraintlayout.widget.ConstraintLayout>